! 1) Set random seed
Random:setSeed = on
Random:seed = 0

! 2) Beam
Beams:idA = 2212            ! first beam, p = 2212, 
Beams:idB = 2212            ! second beam, p = 2212, pbar = -2212
Beams:eCM = 5020.                 ! CM energy of collision

! 3) Hard process
HardQCD:all = on

! 4) Parton shower / MPI / Remnants
PartonLevel:all = on
PartonLevel:MPI = on
PartonLevel:ISR = on
PartonLevel:FSR = on
PartonLevel:Remnants = off
Check:event = off

TimeShower:pTmin = 1

! 5) Hadron level
HadronLevel:all = off

